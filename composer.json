{
    "name": "verbleif/soft-deleteable-extension-bundle",
    "description": "Fixed to work with php 8 attributes.",
    "authors": [{
        "name": "Sherin Bloemendaal",
        "homepage": "https://verbleif.com"
    }],
    "keywords": ["soft-deleteable"],
    "require": {
        "php": ">=8.1",
        "doctrine/orm": "^2.7",
        "gedmo/doctrine-extensions": "^3.4",
        "doctrine/common": "^3.0",
        "symfony/property-access": "^6.0",
        "symfony/framework-bundle": "^6.0"
    },
    "require-dev": {
        "symfony/console": "^6.0"
    },
    "target-dir": "Evence/Bundle/SoftDeleteableExtensionBundle",
    "minimum-stability": "dev",
    "extra": {
        "branch-alias": {
            "dev-master": "0.1-dev"
        }
    },
    "autoload": {
        "psr-0": {
            "Evence\\Bundle\\SoftDeleteableExtensionBundle": ""
        }
    },
    "type": "symfony-bundle"
}
